pipeline {
    agent any

    stages {
        stage('Maven') {
            steps {
                echo 'Building..'
                sh mvn -clean test -D
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}